<Window x:Class="CalendarTaskWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CalendarTaskWPF"
        mc:Ignorable="d"
        Title="CalculatorTaskProgram" Height="500 " Width="800">
    <StackPanel>
        <Label FontSize="20" HorizontalAlignment="Center" VerticalAlignment="Top">
            Calculator Task Program
        </Label>
        <Button Click="Button_Click" Content="Add new task to Calendar" Height="70" Width="200" HorizontalAlignment="Right" Margin="0,0,20,0"/>
        <Button Content="Button" Height="70" Width="200" HorizontalAlignment="Right" Margin="0,10,20,0"/>
    </StackPanel>

</Window>
    
    
